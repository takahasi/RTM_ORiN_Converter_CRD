			<Variable name="M9">
				<Value type="VT_BSTR">
					<bstrVal>NOT_IMPLEMENTED</bstrVal>
				</Value>
			</Variable>
		<!-- *** M10.直交座標系の直線補間(絶対指令) *** -->
			<Variable name="M10">
				<Value type="VT_BSTR">
					<bstrVal>P(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%d)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M11.直交座標系の直線補間(相対指令) *** -->
			<Variable name="M11">
				<Value type="VT_BSTR">
					<bstrVal>P(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%d)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M12.関節座標系の直線補間(直交・絶対指令) *** -->
			<Variable name="M12">
				<Value type="VT_BSTR">
					<bstrVal>P(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%d)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M13.関節座標系の直線補間(直交・相対指令) *** -->
			<Variable name="M13">
				<Value type="VT_BSTR">
					<bstrVal>P(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%d)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M14.関節座標系の直線補間(関節・絶対指令) *** -->
			<Variable name="M14">
				<Value type="VT_BSTR">
					<bstrVal>J(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M15.関節座標系の直線補間(関節・相対指令) *** -->
			<Variable name="M15">
				<Value type="VT_BSTR">
					<bstrVal>J(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M16.グリッパを開く *** -->
			<Variable name="M16">
				<Value type="VT_BSTR">
					<bstrVal>NOT_IMPLEMENTED</bstrVal>
				</Value>
			</Variable>
		<!-- *** M17.動作の一時停止 *** -->
			<Variable name="M17">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M18.動作の再開 *** -->
			<Variable name="M18">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M19.動作の停止 *** -->
			<Variable name="M19">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M20.直交動作時の加速時間を設定 *** -->
			<Variable name="M20">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M21.関節動作時の加速時間を設定 *** -->
			<Variable name="M21">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M22.マニピュレータの設置位置を設定 *** -->
			<Variable name="M22">
				<Value type="VT_BSTR">
					<bstrVal>NOT_IMPLEMENTED</bstrVal>
				</Value>
			</Variable>
		<!-- *** M23.制御点のフランジ面からのオフセット量を設定 *** -->
			<Variable name="M23">
				<Value type="VT_BSTR">
				<bstrVal>NOT_IMPLEMENTED</bstrVal>
				</Value>
			</Variable>
		<!-- *** M24.直交動作時の最大動作速度を設定 *** -->
			<Variable name="M24">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M25.関節動作時の最大動作速度を設定 *** -->
			<Variable name="M25">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M26.直交動作時の最小動作加速時間を設定 *** -->
			<Variable name="M26">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M27.関節動作時の最小動作加速時間を設定 *** -->
			<Variable name="M27">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M28.直交座標系のソフトリミット値を設定 *** -->
			<Variable name="M28">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M29.直交動作時の速度を設定 *** -->
			<Variable name="M29">
				<Value type="VT_BSTR">
					<bstrVal>ExtSpeed</bstrVal>
				</Value>
			</Variable>
		<!-- *** M30.関節動作時の速度を設定 *** -->
			<Variable name="M30">
				<Value type="VT_BSTR">
					<bstrVal>ExtSpeed</bstrVal>
				</Value>
			</Variable>
		<!-- *** M31.直交座標系の円弧補間(絶対指令) *** -->
			<Variable name="M31">
				<Value type="VT_BSTR">
					<bstrVal>P(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%d)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M32.直交座標系の円弧補間(相対指令) *** -->
			<Variable name="M32">
				<Value type="VT_BSTR">
					<bstrVal>P(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%d)</bstrVal>
				</Value>
			</Variable>
		<!-- *** M33.原点復帰時の位置・姿勢を設定 *** -->
			<Variable name="M33">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M34.原点復帰時の位置・姿勢を取得 *** -->
			<Variable name="M34">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** M35.原点復帰動作 *** -->
			<Variable name="M35">
				<Value type="VT_BSTR">
					<bstrVal>J(%.1lf,%.1lf,%.1lf,%.1lf,%.1lf,%.1lf)</bstrVal>
				</Value>
			</Variable>
