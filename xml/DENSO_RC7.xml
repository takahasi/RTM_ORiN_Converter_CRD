<?xml version="1.0" encoding="Shift_JIS"?>
<!--
	***** DENSO NetwoRC Provider *****
-->
<CRD xmlns="http://www.orin.jp/CRD/CRDSchema"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.orin.jp/CRD/CRDSchema CRDSchema.xsd">
	<Controller name="ControllerName">
	<!-- *** CaoProvider *** -->
		<Variable name="ControllerName">
			<Value type="VT_BSTR">
				<bstrVal>NetwoRC</bstrVal>
			</Value>
		</Variable>
		<Variable name="CaoProvName">
			<Value type="VT_BSTR">
				<bstrVal>CaoProv.DENSO.NetwoRC</bstrVal>
			</Value>
		</Variable>
		<Variable name="ConnectionParam">
			<Value type="VT_BSTR">
				<bstrVal>Conn=%s,Timeout=%ld</bstrVal>
			</Value>
		</Variable>
		<Variable name="Manufactur">
			<Value type="VT_BSTR">
				<bstrVal>DENSOWAVE</bstrVal>
			</Value>
		</Variable>
		<Variable name="RobotName">
			<Value type="VT_BSTR">
				<bstrVal>VP6242E-GM</bstrVal>
			</Value>
		</Variable>
		<Variable name="AxisNum">
			<Value type="VT_I2">
				<iVal>6</iVal>
			</Value>
		</Variable>
		<Variable name="AxialType">
			<Value type="VT_ARRAY">
				<array type="VT_BSTR">
					<dimension>1</dimension>
					<arrayBound>
						<lBound>0</lBound>
						<elements>6</elements>
					</arrayBound>
					<arrayData>
						<!-- *** 回転軸:rotation, 直動軸:linear *** -->
						<bstrVal>rotation</bstrVal>
						<bstrVal>rotation</bstrVal>
						<bstrVal>rotation</bstrVal>
						<bstrVal>rotation</bstrVal>
						<bstrVal>rotation</bstrVal>
						<bstrVal>rotation</bstrVal>
					</arrayData>
				</array>
			</Value>
		</Variable>
		<Variable name="IsGripper">
			<Value type="VT_I2">
				<iVal>0</iVal>
			</Value>
		</Variable>
	<!-- *** 未実装の関数には"NOT_IMPLEMENTED"を代入する *** -->
	<!-- *** CommonCommand *** -->
		<!-- *** C1.アラームクリア *** -->
			<Variable name="C1">
				<Value type="VT_BSTR">
					<bstrVal>ClearError</bstrVal>
				</Value>
			</Variable>
		<!-- *** C2.アラーム情報の取得 *** -->
			<Variable name="C2">
				<Value type="VT_BSTR">
					<bstrVal>@ERROR_DESCRIPTION</bstrVal>
				</Value>
			</Variable>
		<!-- *** C3.関節座標系の位置フィードバック情報の取得 *** -->
			<Variable name="C3">
				<Value type="VT_BSTR">
					<bstrVal>@CURRENT_ANGLE</bstrVal>
				</Value>
			</Variable>
		<!-- *** C4.マニピュレータ情報の取得 *** -->
			<Variable name="C4">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** C5.関節座標系のソフトリミット値を取得 *** -->
			<Variable name="C5">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
		<!-- *** C6.ユニットの状態取得 *** -->
			<Variable name="C6">
				<Value type="VT_BSTR">
					<bstrVal>@SERVO_ON</bstrVal>
				</Value>
			</Variable>
		<!-- *** C7.全軸サーボOFF *** -->
			<Variable name="C7">
				<Value type="VT_BSTR">
					<bstrVal>Motor</bstrVal>
				</Value>
			</Variable>
			<Variable name="C7_Option">
				<Value type="VT_BSTR">
					<bstrVal>0</bstrVal>
				</Value>
			</Variable>
		<!-- *** C8.全軸サーボON *** -->
			<Variable name="C8">
				<Value type="VT_BSTR">
					<bstrVal>Motor</bstrVal>
				</Value>
			</Variable>
			<Variable name="C8_Option">
				<Value type="VT_BSTR">
					<bstrVal>1</bstrVal>
				</Value>
			</Variable>
		<!-- *** C9.関節座標系のソフトリミット値設定 *** -->
			<Variable name="C9">
				<Value type="VT_BSTR">
					<bstrVal></bstrVal>
				</Value>
			</Variable>
	<!-- *** MiddleLevelMotionCommand *** -->
		<!-- *** M1.グリッパを閉じる *** -->
			<Variable name="M1">
				<Value type="VT_BSTR">
					<bstrVal>NOT_IMPLEMENTED</bstrVal>
				</Value>
			</Variable>
		<!-- *** M2.マニピュレータの設定位置を取得 *** -->
			<Variable name="M2">
				<Value type="VT_BSTR">
					<bstrVal>NOT_IMPLEMENTED</bstrVal>
				</Value>
			</Variable>
		<!-- *** M3.直交座標系の位置フィードバック情報の取得 *** -->
			<Variable name="M3">
				<Value type="VT_BSTR">
					<bstrVal>@CURRENT_POSITION</bstrVal>
				</Value>
			</Variable>
		<!-- *** M4.直交動作時の最大動作速度を取得 *** -->
			<Variable name="M4">
				<Value type="VT_BSTR">
					<bstrVal>@SPEED</bstrVal>
				</Value>
			</Variable>
		<!-- *** M5.関節動作時の最大動作速度を取得 *** -->
			<Variable name="M5">
				<Value type="VT_BSTR">
					<bstrVal>@SPEED</bstrVal>
				</Value>
			</Variable>
